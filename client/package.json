  "jest": {
    "preset": "ts-jest",
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.mjs$": "babel-jest",
      "^.+\\.[tj]s$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/",
      "public/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "jsx",
      "mjs"
    ]
  }
